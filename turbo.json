{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalEnv": ["*"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".output/**", ".vinxi/**", "dist/**", "build/**"]
		},
		"start": {
			"cache": true,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
